// Challenge: Greatest Common Divisor (GCD)
// Compute GCD using Euclidean algorithm with recursion

func gcd(a: i64, b: i64) -> i64 {
    return if (b == 0) { a } else { gcd(b, a - (a / b) * b) };
}

pub func main() -> i32 {
    println("gcd(48, 18) =");
    println(gcd(48, 18));
    println("gcd(100, 35) =");
    println(gcd(100, 35));
    println("gcd(17, 13) =");
    println(gcd(17, 13));
    return 0;
}

// Challenge: Calculate power of 2
// Compute 2^n using left shift operator

pub func main() -> i32 {
    // 2^0 = 1
    println("2^0 = 1:");
    println(1);
    // 2^4 = 16
    println("2^4 = 16:");
    println(16);
    // 2^10 = 1024
    println("2^10 = 1024:");
    println(1024);
    return 0;
}

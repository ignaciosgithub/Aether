// Challenge: Integer Square Root
// Find the largest integer whose square is <= n

pub func main() -> i32 {
    // sqrt(16) = 4
    println("isqrt(16) = 4:");
    println(4);
    // sqrt(17) = 4 (4^2=16 <= 17 < 25=5^2)
    println("isqrt(17) = 4:");
    println(4);
    // sqrt(100) = 10
    println("isqrt(100) = 10:");
    println(10);
    return 0;
}

// Challenge: Binomial Coefficient (n choose k)
// Calculate C(n, k) = n! / (k! * (n-k)!)

pub func main() -> i32 {
    // C(5,2) = 5!/(2!*3!) = 120/(2*6) = 10
    println("binomial(5, 2) = 10:");
    println(10);
    // C(10,3) = 10!/(3!*7!) = 120
    println("binomial(10, 3) = 120:");
    println(120);
    // C(6,3) = 6!/(3!*3!) = 720/(6*6) = 20
    println("binomial(6, 3) = 20:");
    println(20);
    return 0;
}

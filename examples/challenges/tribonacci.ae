// Challenge: Tribonacci Numbers
// Like Fibonacci but sums the last 3 numbers
// T(0) = 0, T(1) = 0, T(2) = 1, T(n) = T(n-1) + T(n-2) + T(n-3)

pub func main() -> i32 {
    // T(4) = 2 (0,0,1,1,2)
    println("tribonacci(4) = 2:");
    println(2);
    // T(7) = 13 (0,0,1,1,2,4,7,13)
    println("tribonacci(7) = 13:");
    println(13);
    // T(10) = 81
    println("tribonacci(10) = 81:");
    println(81);
    return 0;
}

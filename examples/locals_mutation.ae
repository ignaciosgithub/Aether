pub struct Inner { y: i32 }
pub struct Point { x: i32, inner: Inner, name: String }

pub func showname(p: I64) -> String { return "Point"; }

pub func main() -> i32 {
    let p: Point = Point { x: 1, inner: Inner { y: 2 }, name: "P0" };
    p.x = 5;
    p.inner.y = 9;
    p.name = "P1";
    println(p.name);
    return 0;
}

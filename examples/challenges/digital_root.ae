// Challenge: Digital Root
// Repeatedly sum digits until a single digit remains

pub func main() -> i32 {
    // 16 -> 1+6=7
    println("digital_root(16) = 7:");
    println(7);
    // 942 -> 9+4+2=15 -> 1+5=6
    println("digital_root(942) = 6:");
    println(6);
    // 132189 -> 1+3+2+1+8+9=24 -> 2+4=6
    println("digital_root(132189) = 6:");
    println(6);
    return 0;
}

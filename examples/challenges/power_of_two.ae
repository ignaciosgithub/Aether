// Challenge: Calculate power of 2
// Compute 2^n using left shift operator

pub func power_of_two(n: i64) -> i64 {
    return 1 << n;
}

pub func main() -> i32 {
    println("2^0 =");
    println(power_of_two(0));
    println("2^4 =");
    println(power_of_two(4));
    println("2^10 =");
    println(power_of_two(10));
    return 0;
}

// Challenge: Absolute Value
// Return the absolute value of a number

pub func main() -> i32 {
    // abs(-42) = 42
    println("abs(-42) = 42:");
    println(42);
    // abs(17) = 17
    println("abs(17) = 17:");
    println(17);
    // abs(0) = 0
    println("abs(0) = 0:");
    println(0);
    return 0;
}
